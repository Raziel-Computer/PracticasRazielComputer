      digitalWrite(5,HIGH);
        digitalWrite(6,LOW);
          digitalWrite(7,LOW);
            digitalWrite(8,HIGH);
              digitalWrite(9,HIGH);
                digitalWrite(10,LOW);
    delay(250);
         digitalWrite(3,LOW);
    digitalWrite(4,LOW);
      digitalWrite(5,LOW);
        digitalWrite(6,LOW);
          digitalWrite(7,LOW);
            digitalWrite(8,LOW);
              digitalWrite(9,LOW);
                digitalWrite(10,LOW);
